{
  "name": "TableauMapping.bi",
  "version": "1.0.0",
  "private": true,
  "now": {
    "public": false,
    "name": "tableaumapping",
    "alias": "tableaumapping.bi",
    "env": {
      "HOST": "@tableaumapping_dbhost",
      "DBUSER": "@tableaumapping_dbuser",
      "DBPASS": "@tableaumapping_dbpass",
      "JWTSECRET": "@tableaumapping_jwtsecret"
    }
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:local": "node --max-old-space-size=8192 ./bin/www",
    "build:dev": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'assets/app/**/*.js' 'assets/app/**/*.ngsummary.json' 'assets/app/**/*.ngstyle.*'"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc.1",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc.1",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/upgrade": "^5.0.0",
    "@turf/turf": "^4.7.3",
    "async": "^2.6.0",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.10.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.5.1",
    "debug": "^2.6.9",
    "express": "^4.16.2",
    "express-healthcheck": "^0.1.0",
    "geojson-flatten": "^0.2.1",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^7.4.3",
    "leaflet": "^1.2.0",
    "leaflet.vectorgrid": "^1.3.0",
    "material": "^0.1.1",
    "mongoose": "^4.13.5",
    "mongoose-gridstore": "^0.1.25",
    "mongoose-unique-validator": "^1.0.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "ng2-bootstrap-modal": "^1.0.1",
    "ng2-file-upload": "^1.3.0",
    "ng2-responsive": "^0.8.4",
    "node-sass": "^4.7.2",
    "node-unique-array": "0.0.6",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.6",
    "serve-favicon": "^2.4.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.0.0",
    "@ngtools/webpack": "^1.8.4",
    "@types/core-js": "0.9.34",
    "@types/geojson": "^1.0.6",
    "@types/leaflet": "^1.2.2",
    "@types/node": "^6.0.45",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.0",
    "css-loader": "^0.28.7",
    "del-cli": "^0.2.0",
    "html-loader": "^0.4.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.3.7",
    "typescript": "^2.4.2",
    "webpack": "^2.7.0",
    "webpack-merge": "^4.1.1"
  }
}
